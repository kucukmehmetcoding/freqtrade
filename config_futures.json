{
    "$schema": "https://schema.freqtrade.io/schema.json",
    "max_open_trades": 2,
    "stake_currency": "USDT",
    "stake_amount": 300,
    "tradable_balance_ratio": 0.99,
    "fiat_display_currency": "USD",
    "dry_run": false,
    "cancel_open_orders_on_exit": false,
    "trading_mode": "futures",
    "margin_mode": "isolated",
    "unfilledtimeout": {
        "entry": 10,
        "exit": 10,
        "exit_timeout_count": 0,
        "unit": "minutes"
    },
    "entry_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "exit_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1
    },
    "exchange": {
        "name": "binance",
        "key": "${BINANCE_API_KEY}",
        "secret": "${BINANCE_SECRET}",
        "ccxt_config": {
            "enableRateLimit": true,
            "options": {
                "defaultType": "future"
            }
        },
        "ccxt_async_config": {
            "enableRateLimit": true
        },
        "skip_pair_validation": true,
        "pair_whitelist": [
            "BTC/USDT:USDT",
            "SOL/USDT:USDT",
            "HBAR/USDT:USDT",
            "ADA/USDT:USDT"
        ],
        "pair_blacklist": [
            ".*/UP:USDT",
            ".*/DOWN:USDT"
        ]
    },
    "pairlists": [
        {
            "method": "StaticPairList"
        }
    ],
    "strategy": "DailySwingHunterV5_Futures",
    "strategy_path": "user_data/strategies/",
    "internals": {
        "process_throttle_secs": 5
    },
    "api_server": {
        "enabled": true,
        "listen_ip_address": "0.0.0.0",
        "listen_port": 8089,
        "verbosity": "error",
        "enable_openapi": false,
        "jwt_secret_key": "${JWT_SECRET:-somethingrandom}",
        "CORS_origins": ["*"],
        "username": "${API_USERNAME:-admin}",
        "password": "${API_PASSWORD:-test}"
    }
}
